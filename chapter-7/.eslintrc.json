{
    "plugins": ["jest"],
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": ["standard", "plugin:jest/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "jest/no-commented-out-tests": "off"
    }
}
